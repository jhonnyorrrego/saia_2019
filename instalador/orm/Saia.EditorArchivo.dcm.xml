<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\EditorArchivo" table="editor_archivo">
    <indexes>
      <index name="archivo" columns="archivo"/>
    </indexes>
    <id name="ideditorArchivo" type="integer" column="ideditor_archivo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idfuncionarioEditor" type="integer" column="idfuncionario_editor" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">Funcionario que edita</option>
      </options>
    </field>
    <field name="archivo" type="string" column="archivo" length="600" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Ruta completa del archivo</option>
      </options>
    </field>
    <field name="archivoTemp" type="string" column="archivo_temp" length="600" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="guardado" type="integer" column="guardado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">Si ha guardado o no</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="rama" type="string" column="rama" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Rama a la que pertenece el archivo</option>
        <option name="default">master</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
