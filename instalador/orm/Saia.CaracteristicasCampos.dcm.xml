<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\CaracteristicasCampos" table="caracteristicas_campos">
    <id name="idcaracteristicasCampos" type="integer" column="idcaracteristicas_campos">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipoCaracteristica" type="string" column="tipo_caracteristica" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">validacion</option>
      </options>
    </field>
    <field name="valor" type="string" column="valor" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idcamposFormato" type="integer" column="idcampos_formato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
