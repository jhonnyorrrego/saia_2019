<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\BusquedaIndicador" table="busqueda_indicador">
    <id name="idbusquedaIndicador" type="integer" column="idbusqueda_indicador">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="busquedaIdbusquedaComponente" type="integer" column="busqueda_idbusqueda_componente" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="indicadorIdindicador" type="integer" column="indicador_idindicador" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="text" column="descripcion" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etiqueta" type="string" column="etiqueta" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ancho" type="integer" column="ancho" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="alto" type="integer" column="alto" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
