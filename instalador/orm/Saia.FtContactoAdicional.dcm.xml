<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\FtContactoAdicional" table="ft_contacto_adicional">
    <indexes>
      <index name="i_contacto_adicional_readh" columns="ft_readh"/>
    </indexes>
    <id name="idftContactoAdicional" type="integer" column="idft_contacto_adicional">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ftReadh" type="integer" column="ft_readh" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="identificacion" type="integer" column="identificacion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
