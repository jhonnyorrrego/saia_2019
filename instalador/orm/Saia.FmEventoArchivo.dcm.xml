<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\FmEventoArchivo" table="fm_evento_archivo">
    <indexes>
      <index name="fk_fm_evento_archivo_fm_archivo1_idx" columns="fm_archivo_idarchivo"/>
      <index name="fk_fm_evento_archivo_funcionario1_idx" columns="funcionario_idfuncionario"/>
      <index name="fm_evento_archivo_accion" columns="accion"/>
    </indexes>
    <id name="idfmEventoArchivo" type="integer" column="idfm_evento_archivo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fmArchivoIdarchivo" type="integer" column="fm_archivo_idarchivo" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="accion" type="string" column="accion" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">CREAR,MODIFICAR,ELIMINAR</option>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
  </entity>
</doctrine-mapping>
