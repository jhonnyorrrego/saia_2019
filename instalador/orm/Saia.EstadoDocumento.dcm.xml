<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\EstadoDocumento" table="estado_documento">
    <unique-constraints>
      <unique-constraint name="estado" columns="estado"/>
    </unique-constraints>
    <id name="idestadoDocumento" type="integer" column="idestado_documento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="estado" type="string" column="estado" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Corresponde con documento.estado</option>
      </options>
    </field>
    <field name="enUso" type="integer" column="en_uso" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">indica si el estado se usa en esta version de SAIA</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="descripcion" type="string" column="descripcion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Descripcion del estado</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
