<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\DocumentoVersion" table="documento_version">
    <indexes>
      <index name="i_documento_version_doc" columns="documento_iddocumento"/>
    </indexes>
    <id name="iddocumentoVersion" type="integer" column="iddocumento_version">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numeroVersion" type="integer" column="numero_version" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="prefijo" type="string" column="prefijo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sufijo" type="string" column="sufijo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cargaInicial" type="integer" column="carga_inicial" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="funcionario" type="integer" column="funcionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
