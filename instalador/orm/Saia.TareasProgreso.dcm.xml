<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\TareasProgreso" table="tareas_progreso">
    <id name="idtareasProgreso" type="integer" column="idtareas_progreso">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fkTareasListado" type="integer" column="fk_tareas_listado" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="progreso" type="integer" column="progreso" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaProgreso" type="datetime" column="fecha_progreso" nullable="false"/>
  </entity>
</doctrine-mapping>
