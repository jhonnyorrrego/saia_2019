<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Contador" table="contador">
    <id name="idcontador" type="integer" column="idcontador">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="consecutivo" type="integer" column="consecutivo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="reiniciarCambioAnio" type="integer" column="reiniciar_cambio_anio" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="etiquetaContador" type="string" column="etiqueta_contador" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="post" type="string" column="post" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
