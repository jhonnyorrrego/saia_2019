<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\EntidadCaja" table="entidad_caja">
    <id name="identidadCaja" type="integer" column="identidad_caja">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="entidadIdentidad" type="integer" column="entidad_identidad" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="cajaIdcaja" type="integer" column="caja_idcaja" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="llaveEntidad" type="integer" column="llave_entidad" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="estado" type="string" column="estado" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="permiso" type="string" column="permiso" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
  </entity>
</doctrine-mapping>
