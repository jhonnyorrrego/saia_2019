<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Digitalizacion" table="digitalizacion">
    <indexes>
      <index name="documento_iddocumento" columns="documento_iddocumento"/>
    </indexes>
    <id name="iddigitalizacion" type="integer" column="iddigitalizacion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="accion" type="string" column="accion" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="funcionario" type="string" column="funcionario" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="justificacion" type="text" column="justificacion" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
