<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Indicador" table="indicador">
    <id name="idindicador" type="integer" column="idindicador">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rutaFormulario" type="string" column="ruta_formulario" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etiqueta" type="string" column="etiqueta" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="librerias" type="text" column="librerias" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
