<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\FormatoRuta" table="formato_ruta">
    <id name="idformatoRuta" type="integer" column="idformato_ruta">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="entidad" type="integer" column="entidad" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="llave" type="integer" column="llave" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="firma" type="integer" column="firma" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="orden" type="integer" column="orden" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ruta" type="string" column="ruta" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="funcion" type="string" column="funcion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="formatoIdformato" type="integer" column="formato_idformato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tipoCampo" type="integer" column="tipo_campo" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
