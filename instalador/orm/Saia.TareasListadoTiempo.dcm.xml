<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\TareasListadoTiempo" table="tareas_listado_tiempo">
    <id name="idtareasListadoTiempo" type="integer" column="idtareas_listado_tiempo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fkTareasListado" type="integer" column="fk_tareas_listado" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaRegistro" type="datetime" column="fecha_registro" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="tiempoRegistrado" type="integer" column="tiempo_registrado" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="comentario" type="text" column="comentario" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaInicio" type="date" column="fecha_inicio" nullable="false"/>
    <field name="horaInicio" type="time" column="hora_inicio" nullable="false"/>
    <field name="horaFinal" type="time" column="hora_final" nullable="false"/>
    <field name="estadoAvance" type="string" column="estado_avance" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
