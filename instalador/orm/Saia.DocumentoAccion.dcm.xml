<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\DocumentoAccion" table="documento_accion">
    <indexes>
      <index name="i_documento_accion_doc" columns="documento_iddocumento"/>
    </indexes>
    <id name="iddocumentoAccion" type="integer" column="iddocumento_accion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="accionIdaccion" type="integer" column="accion_idaccion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
  </entity>
</doctrine-mapping>
