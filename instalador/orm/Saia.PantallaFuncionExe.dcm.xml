<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\PantallaFuncionExe" table="pantalla_funcion_exe">
    <indexes>
      <index name="fk_pantalla_funcion_exe_pantalla1_idx" columns="pantalla_idpantalla"/>
      <index name="fk_pantalla_funcion_exe_pantalla_funcion1_idx" columns="fk_idpantalla_funcion"/>
    </indexes>
    <id name="idpantallaFuncionExe" type="integer" column="idpantalla_funcion_exe">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="accion" type="string" column="accion" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="momento" type="integer" column="momento" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">1,anterior;2,posterior</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="vistas" type="string" column="vistas" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">a=adicionar,e=editar,d=eliminar,v=mostrar,s=buscar,l=listar</option>
        <option name="default">v</option>
      </options>
    </field>
    <field name="orden" type="integer" column="orden" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="pantallaIdpantalla" type="integer" column="pantalla_idpantalla" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fkIdpantallaFuncion" type="integer" column="fk_idpantalla_funcion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
