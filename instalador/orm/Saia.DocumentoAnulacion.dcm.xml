<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\DocumentoAnulacion" table="documento_anulacion">
    <indexes>
      <index name="i_documento_anulacion_doc" columns="documento_iddocumento"/>
    </indexes>
    <id name="iddocumentoAnulacion" type="integer" column="iddocumento_anulacion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="funcionario" type="string" column="funcionario" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaSolicitud" type="date" column="fecha_solicitud" nullable="false"/>
    <field name="descripcion" type="string" column="descripcion" length="3000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="string" column="estado" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaAnulacion" type="date" column="fecha_anulacion" nullable="true"/>
  </entity>
</doctrine-mapping>
