<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Dependencia" table="dependencia">
    <id name="iddependencia" type="integer" column="iddependencia">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codigo" type="string" column="codigo" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="fechaIngreso" type="datetime" column="fecha_ingreso" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="codPadre" type="integer" column="cod_padre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tipo" type="integer" column="tipo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="estado" type="boolean" column="estado" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="codigoTabla" type="string" column="codigo_tabla" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="extension" type="string" column="extension" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ubicacionDependencia" type="text" column="ubicacion_dependencia" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="logo" type="blob" column="logo" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="orden" type="string" column="orden" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
