<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Diagramdata" table="diagramdata">
    <id name="type" type="string" column="type"/>
    <id name="diagramid" type="integer" column="diagramId"/>
    <field name="filename" type="string" column="fileName" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="filesize" type="integer" column="fileSize" nullable="true">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">The size of diagram in bytes</option>
      </options>
    </field>
    <field name="lastupdate" type="datetime" column="lastUpdate" nullable="false"/>
  </entity>
</doctrine-mapping>
