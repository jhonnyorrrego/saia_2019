      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIdpasoDocumento" type="integer" column="documento_idpaso_documento" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="instanciaIdpasoInstancia" type="integer" column="instancia_idpaso_instancia" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="funcionarioCodigo" type="integer" column="funcionario_codigo" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaJustificacion" type="datetime" column="fecha_justificacion" nullable="false"/>
    <field name="observaciones" type="text" column="observaciones" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
