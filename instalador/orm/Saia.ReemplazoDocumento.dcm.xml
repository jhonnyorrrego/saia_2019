<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\ReemplazoDocumento" table="reemplazo_documento">
    <indexes>
      <index name="i_reemplazo_documento_documento_" columns="documento_iddocumento"/>
    </indexes>
    <id name="idreemplazoDocumento" type="integer" column="idreemplazo_documento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fkIdreemplazoSaia" type="integer" column="fk_idreemplazo_saia" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">Verificar si se puede eliminar el campo</option>
      </options>
    </field>
    <field name="tipoReemplazoDoc" type="integer" column="tipo_reemplazo_doc" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">1,ruta;2,asignacion</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
