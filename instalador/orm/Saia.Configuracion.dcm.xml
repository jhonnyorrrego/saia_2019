<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Configuracion" table="configuracion">
    <id name="idconfiguracion" type="integer" column="idconfiguracion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="valor" type="string" column="valor" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="encrypt" type="integer" column="encrypt" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
