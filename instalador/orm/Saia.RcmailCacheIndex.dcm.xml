<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\RcmailCacheIndex" table="rcmail_cache_index">
    <indexes>
      <index name="expires_index" columns="expires"/>
    </indexes>
    <id name="userId" type="integer" column="user_id"/>
    <id name="mailbox" type="string" column="mailbox" length="255"/>
    <field name="expires" type="datetime" column="expires" nullable="true"/>
    <field name="valid" type="boolean" column="valid" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="data" type="text" column="data" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
