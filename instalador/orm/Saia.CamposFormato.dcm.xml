<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\CamposFormato" table="campos_formato">
    <indexes>
      <index name="formato_idformato" columns="formato_idformato"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="ix_campos_formato_formato" columns="formato_idformato,nombre"/>
    </unique-constraints>
    <id name="idcamposFormato" type="integer" column="idcampos_formato">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="formatoIdformato" type="integer" column="formato_idformato" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="etiqueta" type="string" column="etiqueta" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="tipoDato" type="string" column="tipo_dato" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="longitud" type="string" column="longitud" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="obligatoriedad" type="boolean" column="obligatoriedad" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="valor" type="text" column="valor" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="acciones" type="string" column="acciones" length="10" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">a,e,b</option>
      </options>
    </field>
    <field name="ayuda" type="text" column="ayuda" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="predeterminado" type="string" column="predeterminado" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="banderas" type="string" column="banderas" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etiquetaHtml" type="string" column="etiqueta_html" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">text</option>
      </options>
    </field>
    <field name="orden" type="boolean" column="orden" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="mascara" type="string" column="mascara" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adicionales" type="string" column="adicionales" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="autoguardado" type="integer" column="autoguardado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="filaVisible" type="integer" column="fila_visible" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
