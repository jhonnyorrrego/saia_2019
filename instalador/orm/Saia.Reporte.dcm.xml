<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Reporte" table="reporte">
    <id name="idreporte" type="integer" column="idreporte">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="sqlReporte" type="string" column="sql_reporte" length="3000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombreArchivo" type="string" column="nombre_archivo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">reporte</option>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="mascaras" type="string" column="mascaras" length="3000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="moduloIdmodulo" type="integer" column="modulo_idmodulo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="camposTexto" type="string" column="campos_texto" length="2000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="camposNumero" type="string" column="campos_numero" length="2000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipoOrdenamiento" type="string" column="tipo_ordenamiento" length="4" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="campoOrdenamiento" type="string" column="campo_ordenamiento" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
