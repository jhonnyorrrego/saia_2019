      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="regionalIdregional" type="integer" column="regional_idregional" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="directorioIddirectorio" type="integer" column="directorio_iddirectorio" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="propietario" type="integer" column="propietario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
