<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\FmDirectorio" table="fm_directorio">
    <indexes>
      <index name="fk_directorio_funcionario_idx" columns="funcionario_idfuncionario"/>
      <index name="fk_directorio_regional1_idx" columns="regional_idregional"/>
      <index name="fk_fm_directorio_fm_directorio1_idx" columns="directorio_iddirectorio"/>
      <index name="fk_fm_directorio_funcionario1_idx" columns="propietario"/>
    </indexes>
    <id name="iddirectorio" type="integer" column="iddirectorio">
      <generator strategy="IDENTITY"/>
