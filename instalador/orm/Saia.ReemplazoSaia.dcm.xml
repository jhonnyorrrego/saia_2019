<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\ReemplazoSaia" table="reemplazo_saia">
    <id name="idreemplazoSaia" type="integer" column="idreemplazo_saia">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="antiguo" type="integer" column="antiguo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="nuevo" type="integer" column="nuevo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fechaInicio" type="date" column="fecha_inicio" nullable="false"/>
    <field name="fechaFin" type="date" column="fecha_fin" nullable="true"/>
    <field name="cargoNuevo" type="integer" column="cargo_nuevo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="estado" type="string" column="estado" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="tipoReemplazo" type="integer" column="tipo_reemplazo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">1,Temporal;2,definitivo</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="observaciones" type="text" column="observaciones" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
