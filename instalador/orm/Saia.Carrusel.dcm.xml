<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Carrusel" table="carrusel">
    <id name="idcarrusel" type="integer" column="idcarrusel">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="autoplay" type="string" column="autoplay" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="delay" type="string" column="delay" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">3000</option>
      </options>
    </field>
    <field name="animationtime" type="string" column="animationtime" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">600</option>
      </options>
    </field>
    <field name="easing" type="string" column="easing" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaInicio" type="date" column="fecha_inicio" nullable="false"/>
    <field name="fechaFin" type="date" column="fecha_fin" nullable="false"/>
  </entity>
</doctrine-mapping>
