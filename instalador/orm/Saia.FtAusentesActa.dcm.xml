      <index name="i_ausentes_acta_serie_idse" columns="serie_idserie"/>
    </indexes>
    <id name="idftAusentesActa" type="integer" column="idft_ausentes_acta">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ftActa" type="integer" column="ft_acta" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="funcionarioAusente" type="string" column="funcionario_ausente" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="justificada" type="integer" column="justificada" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="serieIdserie" type="integer" column="serie_idserie" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
