<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Serie" table="serie">
    <indexes>
      <index name="cod_padre" columns="cod_padre"/>
      <index name="Indice_llave_entidad" columns="llave_entidad"/>
      <index name="serie_idserie_PK" columns="idserie"/>
    </indexes>
    <id name="idserie" type="integer" column="idserie">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="codPadre" type="integer" column="cod_padre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="diasEntrega" type="integer" column="dias_entrega" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">8</option>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipoEntidad" type="integer" column="tipo_entidad" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="llaveEntidad" type="string" column="llave_entidad" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="retencionGestion" type="boolean" column="retencion_gestion" nullable="false">
      <options>
        <option name="default">3</option>
      </options>
    </field>
    <field name="retencionCentral" type="boolean" column="retencion_central" nullable="false">
      <options>
        <option name="default">5</option>
      </options>
    </field>
    <field name="conservacion" type="string" column="conservacion" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="digitalizacion" type="boolean" column="digitalizacion" nullable="true"/>
    <field name="seleccion" type="boolean" column="seleccion" nullable="true"/>
    <field name="otro" type="string" column="otro" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="procedimiento" type="text" column="procedimiento" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="copia" type="boolean" column="copia" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tipo" type="boolean" column="tipo" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="clase" type="boolean" column="clase" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="estado" type="boolean" column="estado" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="categoria" type="integer" column="categoria" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">2</option>
      </options>
    </field>
    <field name="orden" type="string" column="orden" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipoExpediente" type="string" column="tipo_expediente" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tvd" type="integer" column="tvd" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
