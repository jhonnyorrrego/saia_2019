<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Modulo" table="modulo">
    <indexes>
      <index name="i_modulo_destino" columns="destino"/>
    </indexes>
    <id name="idmodulo" type="integer" column="idmodulo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="perteneceNucleo" type="integer" column="pertenece_nucleo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">secundario</option>
      </options>
    </field>
    <field name="imagen" type="string" column="imagen" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">botones/configuracion/default.gif</option>
      </options>
    </field>
    <field name="etiqueta" type="string" column="etiqueta" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="enlace" type="string" column="enlace" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="enlaceMobil" type="string" column="enlace_mobil" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="destino" type="string" column="destino" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">centro</option>
      </options>
    </field>
    <field name="codPadre" type="integer" column="cod_padre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="orden" type="boolean" column="orden" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="ayuda" type="text" column="ayuda" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parametros" type="string" column="parametros" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="busquedaIdbusqueda" type="integer" column="busqueda_idbusqueda" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="permisoAdmin" type="boolean" column="permiso_admin" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="busqueda" type="string" column="busqueda" length="5" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="enlacePantalla" type="integer" column="enlace_pantalla" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
