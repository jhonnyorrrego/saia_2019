<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\DiagramInstance" table="diagram_instance">
    <id name="iddiagramInstance" type="integer" column="iddiagram_instance">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="diagramIddiagram" type="integer" column="diagram_iddiagram" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="funcionarioCodigo" type="integer" column="funcionario_codigo" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="estadoDiagramInstance" type="integer" column="estado_diagram_instance" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">1,Ejecutado;2,Cerrado,3,Cancelado;4,Pendiente;5,Atrasado;6,Iniciado</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
