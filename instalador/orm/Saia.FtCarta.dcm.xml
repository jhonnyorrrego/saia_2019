    <id name="idftCarta" type="integer" column="idft_carta">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="contenido" type="text" column="contenido" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="copiainterna" type="text" column="copiainterna" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaCarta" type="date" column="fecha_carta" nullable="false"/>
    <field name="anexosFisicos" type="text" column="anexos_fisicos" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="iniciales" type="string" column="iniciales" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="copia" type="text" column="copia" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="despedida" type="string" column="despedida" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="serieIdserie" type="integer" column="serie_idserie" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="destinos" type="text" column="destinos" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="asunto" type="string" column="asunto" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="anexosDigitales" type="string" column="anexos_digitales" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vercopiainterna" type="string" column="vercopiainterna" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="dependencia" type="integer" column="dependencia" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="encabezado" type="integer" column="encabezado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="firma" type="integer" column="firma" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="variosRadicados" type="integer" column="varios_radicados" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idflujo" type="string" column="idflujo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">4</option>
      </options>
    </field>
    <field name="tipoCopiaInterna" type="integer" column="tipo_copia_interna" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">2</option>
      </options>
    </field>
    <field name="versionCarta" type="string" column="version_carta" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
