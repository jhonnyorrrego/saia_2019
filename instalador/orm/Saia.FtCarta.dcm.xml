      <options>
        <option name="unsigned"/>
        <option name="default">2</option>
      </options>
    </field>
    <field name="estadoDocumento" type="integer" column="estado_documento" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="expedienteSerie" type="integer" column="expediente_serie" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
