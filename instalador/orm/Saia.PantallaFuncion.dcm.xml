<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\PantallaFuncion" table="pantalla_funcion">
    <id name="idpantallaFuncion" type="integer" column="idpantalla_funcion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="parametros" type="string" column="parametros" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fkIdpantallaLibreria" type="integer" column="fk_idpantalla_libreria" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tipoFuncion" type="string" column="tipo_funcion" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ayuda" type="text" column="ayuda" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
