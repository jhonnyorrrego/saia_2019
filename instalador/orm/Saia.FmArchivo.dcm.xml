<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\FmArchivo" table="fm_archivo">
    <indexes>
      <index name="fk_archivo_directorio1_idx" columns="directorio_iddirectorio"/>
      <index name="fk_archivo_funcionario1_idx" columns="funcionario_idfuncionario"/>
      <index name="archivo_nombre_idx" columns="nombre"/>
    </indexes>
    <id name="idarchivo" type="integer" column="idarchivo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="directorioIddirectorio" type="integer" column="directorio_iddirectorio" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="formato" type="string" column="formato" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="peso" type="integer" column="peso" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
  </entity>
</doctrine-mapping>
