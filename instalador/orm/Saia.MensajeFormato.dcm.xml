<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\MensajeFormato" table="mensaje_formato">
    <id name="idmensajeFormato" type="integer" column="idmensaje_formato">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="formatoIdformato" type="integer" column="formato_idformato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="campoMensaje" type="string" column="campo_mensaje" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="campoFormato" type="string" column="campo_formato" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
