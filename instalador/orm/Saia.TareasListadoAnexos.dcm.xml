<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\TareasListadoAnexos" table="tareas_listado_anexos">
    <id name="idtareasListadoAnexos" type="integer" column="idtareas_listado_anexos">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="etiqueta" type="string" column="etiqueta" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ruta" type="string" column="ruta" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fkTareasListado" type="integer" column="fk_tareas_listado" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaHora" type="datetime" column="fecha_hora" nullable="false"/>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
