<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\RcmailContactgroups" table="rcmail_contactgroups">
    <indexes>
      <index name="contactgroups_user_index" columns="user_id,del"/>
    </indexes>
    <id name="contactgroupId" type="integer" column="contactgroup_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="changed" type="datetime" column="changed" nullable="false">
      <options>
        <option name="default">1000-01-01 00:00:00</option>
      </options>
    </field>
    <field name="del" type="boolean" column="del" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="name" type="string" column="name" length="128" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
