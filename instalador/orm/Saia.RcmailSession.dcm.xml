<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\RcmailSession" table="rcmail_session">
    <indexes>
      <index name="changed_index" columns="changed"/>
    </indexes>
    <id name="sessId" type="string" column="sess_id" length="128">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">1000-01-01 00:00:00</option>
      </options>
    </field>
    <field name="changed" type="datetime" column="changed" nullable="false">
      <options>
        <option name="default">1000-01-01 00:00:00</option>
      </options>
    </field>
    <field name="ip" type="string" column="ip" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vars" type="text" column="vars" length="16777215" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
