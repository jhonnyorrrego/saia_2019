<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\FtFuncionariosRuta" table="ft_funcionarios_ruta">
    <indexes>
      <index name="i_funcionarios_ruta_ruta_distr" columns="ft_ruta_distribucion"/>
      <index name="i_funcionarios_ruta_serie_idse" columns="serie_idserie"/>
    </indexes>
    <id name="idftFuncionariosRuta" type="integer" column="idft_funcionarios_ruta">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mensajeroRuta" type="string" column="mensajero_ruta" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ftRutaDistribucion" type="integer" column="ft_ruta_distribucion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaMensajero" type="date" column="fecha_mensajero" nullable="false"/>
    <field name="estadoMensajero" type="integer" column="estado_mensajero" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="serieIdserie" type="integer" column="serie_idserie" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
