    <field name="dependencia" type="integer" column="dependencia" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="encabezado" type="integer" column="encabezado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="firma" type="integer" column="firma" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="estadoDocumento" type="integer" column="estado_documento" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
