      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="motivoConsulta" type="integer" column="motivo_consulta" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="descripcionCita" type="text" column="descripcion_cita" length="65535" nullable="true">
