<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\PrioridadDocumento" table="prioridad_documento">
    <indexes>
      <index name="i_prioridad_documento_documento_" columns="documento_iddocumento"/>
    </indexes>
    <id name="idprioridadDocumento" type="integer" column="idprioridad_documento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaAsignacion" type="datetime" column="fecha_asignacion" nullable="false"/>
    <field name="prioridad" type="integer" column="prioridad" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
