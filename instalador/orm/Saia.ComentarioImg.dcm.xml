<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\ComentarioImg" table="comentario_img">
    <indexes>
      <index name="i_comentario_img_doc" columns="documento_iddocumento"/>
    </indexes>
    <id name="idcomentarioImg" type="integer" column="idcomentario_img">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">PAGINA</option>
      </options>
    </field>
    <field name="pagina" type="integer" column="pagina" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="comentario" type="text" column="comentario" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="posx" type="integer" column="posx" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="posy" type="integer" column="posy" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="funcionario" type="string" column="funcionario" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
  </entity>
</doctrine-mapping>
