<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\VersionDocumento" table="version_documento">
    <indexes>
      <index name="i_version_documento_documento_" columns="documento_iddocumento"/>
    </indexes>
    <id name="idversionDocumento" type="integer" column="idversion_documento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="version" type="integer" column="version" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pdf" type="string" column="pdf" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
