<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\PantallaComponente" table="pantalla_componente">
    <id name="idpantallaComponente" type="integer" column="idpantalla_componente">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etiqueta" type="string" column="etiqueta" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clase" type="string" column="clase" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="componente" type="text" column="componente" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="opciones" type="text" column="opciones" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="procesar" type="string" column="procesar" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="categoria" type="string" column="categoria" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">I</option>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="librerias" type="string" column="librerias" length="3999" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipoComponente" type="integer" column="tipo_componente" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="eliminar" type="string" column="eliminar" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
