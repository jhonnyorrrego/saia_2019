      <generator strategy="IDENTITY"/>
    </id>
    <field name="tareasIdtareas" type="integer" column="tareas_idtareas" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="descripcion" type="text" column="descripcion" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ejecutor" type="integer" column="ejecutor" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
