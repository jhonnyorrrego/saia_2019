<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\PantallaInclude" table="pantalla_include">
    <indexes>
      <index name="fk_pantalla_include_pantalla_idx" columns="pantalla_idpantalla"/>
      <index name="fk_pantalla_include_pantalla_libreria1_idx" columns="fk_idpantalla_libreria"/>
    </indexes>
    <id name="idpantallaInclude" type="integer" column="idpantalla_include">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="orden" type="integer" column="orden" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lugarIncluir" type="string" column="lugar_incluir" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">footer</option>
      </options>
    </field>
    <field name="accionesInclude" type="string" column="acciones_include" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">a,e,m</option>
      </options>
    </field>
    <field name="pantallaIdpantalla" type="integer" column="pantalla_idpantalla" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fkIdpantallaLibreria" type="integer" column="fk_idpantalla_libreria" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
