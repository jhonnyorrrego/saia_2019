<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\FuncionarioEditor" table="funcionario_editor">
    <id name="idfuncionario" type="integer" column="idfuncionario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="apellidos" type="string" column="apellidos" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clave" type="string" column="clave" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailContrasena" type="string" column="email_contrasena" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="fechaIngreso" type="datetime" column="fecha_ingreso" nullable="false"/>
    <field name="funcionarioCodigo" type="integer" column="funcionario_codigo" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="login" type="string" column="login" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombres" type="string" column="nombres" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="1" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ultimoPwd" type="datetime" column="ultimo_pwd" nullable="false"/>
  </entity>
</doctrine-mapping>
