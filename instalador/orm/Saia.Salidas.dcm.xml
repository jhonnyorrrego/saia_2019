<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Salidas" table="salidas">
    <indexes>
      <index name="documento_iddocumento" columns="documento_iddocumento"/>
    </indexes>
    <id name="idsalida" type="integer" column="idsalida">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="numeroGuia" type="string" column="numero_guia" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="empresa" type="integer" column="empresa" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="responsable" type="integer" column="responsable" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
    <field name="fechaDespacho" type="date" column="fecha_despacho" nullable="true"/>
    <field name="tipoDespacho" type="string" column="tipo_despacho" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notas" type="text" column="notas" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="string" column="estado" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="radicadoDespacho" type="integer" column="radicado_despacho" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
