<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Permiso" table="permiso">
    <id name="idpermiso" type="integer" column="idpermiso">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="accion" type="integer" column="accion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="moduloIdmodulo" type="integer" column="modulo_idmodulo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="caracteristicaPropio" type="string" column="caracteristica_propio" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="caracteristicaGrupo" type="string" column="caracteristica_grupo" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="caracteristicaTotal" type="string" column="caracteristica_total" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="boolean" column="tipo" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
