<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\FuncionesFormatoAccion" table="funciones_formato_accion">
    <id name="idfuncionesFormatoAccion" type="integer" column="idfunciones_formato_accion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idfuncionesFormato" type="integer" column="idfunciones_formato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="accionIdaccion" type="integer" column="accion_idaccion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="formatoIdformato" type="integer" column="formato_idformato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="momento" type="string" column="momento" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">ANTERIOR</option>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="orden" type="integer" column="orden" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
