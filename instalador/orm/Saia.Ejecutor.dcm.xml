<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Ejecutor" table="ejecutor">
    <indexes>
      <index name="nombre" columns="nombre"/>
    </indexes>
    <id name="idejecutor" type="integer" column="idejecutor">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="identificacion" type="string" column="identificacion" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="fechaIngreso" type="datetime" column="fecha_ingreso" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="tipoEjecutor" type="integer" column="tipo_ejecutor" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
