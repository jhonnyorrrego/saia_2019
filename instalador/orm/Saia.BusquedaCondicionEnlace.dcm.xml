<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\BusquedaCondicionEnlace" table="busqueda_condicion_enlace">
    <id name="idbusquedaCondicionEnlace" type="integer" column="idbusqueda_condicion_enlace">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fkBusquedaCondicion" type="integer" column="fk_busqueda_condicion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="codPadre" type="integer" column="cod_padre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="comparacion" type="string" column="comparacion" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="orden" type="integer" column="orden" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
