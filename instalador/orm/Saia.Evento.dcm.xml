<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Evento" table="evento">
    <indexes>
      <index name="realiza" columns="funcionario_codigo"/>
    </indexes>
    <id name="idevento" type="integer" column="idevento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="funcionarioCodigo" type="string" column="funcionario_codigo" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
    <field name="evento" type="string" column="evento" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">ADICIONAR</option>
      </options>
    </field>
    <field name="tablaE" type="string" column="tabla_e" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="string" column="estado" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="registroId" type="integer" column="registro_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="detalle" type="text" column="detalle" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigoSql" type="text" column="codigo_sql" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
