<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Expediente" table="expediente">
    <indexes>
      <index name="fk_idcaja" columns="fk_idcaja"/>
      <index name="serie_idserie" columns="serie_idserie"/>
    </indexes>
    <id name="idexpediente" type="integer" column="idexpediente">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="descripcion" type="text" column="descripcion" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigo" type="string" column="codigo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codPadre" type="integer" column="cod_padre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fkIdcaja" type="integer" column="fk_idcaja" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="propietario" type="string" column="propietario" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="serieIdserie" type="integer" column="serie_idserie" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="codArbol" type="string" column="cod_arbol" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigoNumero" type="string" column="codigo_numero" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fondo" type="string" column="fondo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="proceso" type="string" column="proceso" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaExtremaI" type="date" column="fecha_extrema_i" nullable="true"/>
    <field name="fechaExtremaF" type="date" column="fecha_extrema_f" nullable="true"/>
    <field name="noUnidadConservacion" type="string" column="no_unidad_conservacion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noFolios" type="string" column="no_folios" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noCarpeta" type="string" column="no_carpeta" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="soporte" type="integer" column="soporte" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="frecuenciaConsulta" type="integer" column="frecuencia_consulta" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ubicacion" type="integer" column="ubicacion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="unidadAdmin" type="string" column="unidad_admin" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rutaQr" type="string" column="ruta_qr" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoArchivo" type="integer" column="estado_archivo" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">1,Gestion;2,Central;3,Historico</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="estadoCierre" type="integer" column="estado_cierre" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">1,Abierto;2,Cerrado</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="fechaCierre" type="date" column="fecha_cierre" nullable="true"/>
    <field name="funcionarioCierre" type="integer" column="funcionario_cierre" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">idfuncionario</option>
      </options>
    </field>
    <field name="proxEstadoArchivo" type="integer" column="prox_estado_archivo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="notasTransf" type="text" column="notas_transf" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tomoPadre" type="integer" column="tomo_padre" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tomoNo" type="integer" column="tomo_no" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="agrupador" type="integer" column="agrupador" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="indiceUno" type="string" column="indice_uno" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="indiceDos" type="string" column="indice_dos" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="indiceTres" type="string" column="indice_tres" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dependenciaIddependencia" type="integer" column="dependencia_iddependencia" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
