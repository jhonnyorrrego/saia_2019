<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Almacenamiento" table="almacenamiento">
    <indexes>
      <index name="i_almacenamiento_doc" columns="documento_iddocumento"/>
    </indexes>
    <id name="idalmacenamiento" type="integer" column="idalmacenamiento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="folderIdfolder" type="integer" column="folder_idfolder" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="soporte" type="string" column="soporte" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numFolios" type="integer" column="num_folios" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="anexos" type="string" column="anexos" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deterioro" type="string" column="deterioro" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="responsable" type="integer" column="responsable" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="registroEntrada" type="datetime" column="registro_entrada" nullable="false"/>
  </entity>
</doctrine-mapping>
