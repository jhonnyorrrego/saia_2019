<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Funcionario" table="funcionario">
    <id name="idfuncionario" type="integer" column="idfuncionario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="funcionarioCodigo" type="integer" column="funcionario_codigo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="login" type="string" column="login" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="nombres" type="string" column="nombres" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidos" type="string" column="apellidos" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firma" type="blob" column="firma" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firmaTemporal" type="blob" column="firma_temporal" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firmaOriginal" type="blob" column="firma_original" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="fechaIngreso" type="datetime" column="fecha_ingreso" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="clave" type="string" column="clave" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="nit" type="string" column="nit" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="perfil" type="string" column="perfil" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">6</option>
      </options>
    </field>
    <field name="debeFirmar" type="boolean" column="debe_firmar" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="1" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ultimoPwd" type="datetime" column="ultimo_pwd" nullable="true"/>
    <field name="mensajeria" type="string" column="mensajeria" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sistema" type="boolean" column="sistema" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="emailContrasena" type="string" column="email_contrasena" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaFinInactivo" type="date" column="fecha_fin_inactivo" nullable="true"/>
    <field name="intentoLogin" type="integer" column="intento_login" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fotoOriginal" type="text" column="foto_original" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fotoRecorte" type="text" column="foto_recorte" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fotoCordenadas" type="string" column="foto_cordenadas" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
