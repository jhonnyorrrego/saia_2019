<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\EntidadPretexto" table="entidad_pretexto">
    <id name="identidadPretexto" type="integer" column="identidad_pretexto">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pretextoIdpretexto" type="integer" column="pretexto_idpretexto" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="entidadIdentidad" type="integer" column="entidad_identidad" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="llaveEntidad" type="integer" column="llave_entidad" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
  </entity>
</doctrine-mapping>
