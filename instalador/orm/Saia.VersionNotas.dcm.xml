<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\VersionNotas" table="version_notas">
    <id name="idversionNotas" type="integer" column="idversion_notas">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="observaciones" type="text" column="observaciones" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fkIdversionDocumento" type="integer" column="fk_idversion_documento" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
