<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\EntidadSerie" table="entidad_serie">
    <indexes>
      <index name="i_entidad_serie_serie_idse" columns="serie_idserie"/>
    </indexes>
    <id name="identidadSerie" type="integer" column="identidad_serie">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="entidadIdentidad" type="integer" column="entidad_identidad" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="serieIdserie" type="integer" column="serie_idserie" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="llaveEntidad" type="integer" column="llave_entidad" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="estado" type="string" column="estado" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="1" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
  </entity>
</doctrine-mapping>
