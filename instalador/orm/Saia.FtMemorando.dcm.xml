      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="expedienteSerie" type="string" column="expediente_serie" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailAprobar" type="integer" column="email_aprobar" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">2</option>
      </options>
    </field>
