<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\PasoDocumento" table="paso_documento">
    <indexes>
      <index name="i_paso_documento_documento_" columns="documento_iddocumento"/>
    </indexes>
    <id name="idpasoDocumento" type="integer" column="idpaso_documento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pasoIdpaso" type="integer" column="paso_idpaso" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaAsignacion" type="datetime" column="fecha_asignacion" nullable="false"/>
    <field name="diagramIddiagramInstance" type="integer" column="diagram_iddiagram_instance" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="estadoPasoDocumento" type="integer" column="estado_paso_documento" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">1,Ejecutado;2,Cerrado,3,Cancelado;4,Pendiente;5,Atrasado;6,Iniciado</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fechaLimite" type="datetime" column="fecha_limite" nullable="true"/>
  </entity>
</doctrine-mapping>
