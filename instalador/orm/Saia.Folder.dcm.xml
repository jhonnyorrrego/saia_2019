<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\Folder" table="folder">
    <indexes>
      <index name="i_folder_serie_idse" columns="serie_idserie"/>
    </indexes>
    <id name="idfolder" type="integer" column="idfolder">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cajaIdcaja" type="integer" column="caja_idcaja" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="unidadAdmin" type="string" column="unidad_admin" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subseccionI" type="string" column="subseccion_i" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subseccionIi" type="string" column="subseccion_ii" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numeroOrden" type="string" column="numero_orden" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombreExpediente" type="text" column="nombre_expediente" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noTomo" type="string" column="no_tomo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigoNumero" type="string" column="codigo_numero" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fondo" type="string" column="fondo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="serieIdserie" type="integer" column="serie_idserie" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fechaExtremaI" type="datetime" column="fecha_extrema_i" nullable="false"/>
    <field name="fechaExtremaF" type="datetime" column="fecha_extrema_f" nullable="false"/>
    <field name="noUnidadConservacion" type="string" column="no_unidad_conservacion" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noFolios" type="string" column="no_folios" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="noCarpeta" type="string" column="no_carpeta" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="soporte" type="integer" column="soporte" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">1:CD-ROM,2:DISKETE,3:DVD,4:DOCUMENTO,5:FAX,6:REVISTA O LIBRO,7:VIDEO,8:OTROS ANEXOS</option>
      </options>
    </field>
    <field name="frecuenciaConsulta" type="integer" column="frecuencia_consulta" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">1:Alta,2:Media,3:Baja</option>
      </options>
    </field>
    <field name="ubicacion" type="integer" column="ubicacion" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">1:Central,2:gestion,3:historico</option>
      </options>
    </field>
    <field name="funcionarioIdfuncionario" type="integer" column="funcionario_idfuncionario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
