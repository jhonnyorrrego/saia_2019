<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\ListaNegraAcceso" table="lista_negra_acceso">
    <id name="idlistaNegraAcceso" type="integer" column="idlista_negra_acceso">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="login" type="string" column="login" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="iplocal" type="string" column="iplocal" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ipremota" type="string" column="ipremota" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="true"/>
    <field name="estado" type="integer" column="estado" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
