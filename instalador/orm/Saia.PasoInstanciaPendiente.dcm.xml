<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\PasoInstanciaPendiente" table="paso_instancia_pendiente">
    <indexes>
      <index name="documento_iddocumento" columns="documento_iddocumento"/>
    </indexes>
    <id name="idpasoInstancia" type="integer" column="idpaso_instancia">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="actividadIdpasoActividad" type="integer" column="actividad_idpaso_actividad" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="responsable" type="text" column="responsable" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Quien debe realizar la accion funcionario</option>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="documentoIddocumento" type="integer" column="documento_iddocumento" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
