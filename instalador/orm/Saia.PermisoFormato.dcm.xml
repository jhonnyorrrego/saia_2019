      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="idpropietario" type="integer" column="idpropietario" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="caracteristicaPropio" type="string" column="caracteristica_propio" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="caracteristicaDependencia" type="string" column="caracteristica_dependencia" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="caracteristicaCargo" type="string" column="caracteristica_cargo" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="caracteristicaTotal" type="string" column="caracteristica_total" length="8" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
