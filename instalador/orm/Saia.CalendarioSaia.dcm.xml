<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\CalendarioSaia" table="calendario_saia">
    <id name="idcalendarioSaia" type="integer" column="idcalendario_saia">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etiqueta" type="string" column="etiqueta" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
    <field name="tipo" type="integer" column="tipo" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="estilo" type="string" column="estilo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datos" type="string" column="datos" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="encabezadoIzquierda" type="string" column="encabezado_izquierda" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="encabezadoCentro" type="string" column="encabezado_centro" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="encabezadoDerecho" type="string" column="encabezado_derecho" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adicionarEvento" type="string" column="adicionar_evento" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="busquedaAvanzada" type="string" column="busqueda_avanzada" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
