<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Saia\BusquedaGrafico" table="busqueda_grafico">
    <id name="idbusquedaGrafico" type="integer" column="idbusqueda_grafico">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="busquedaIdbusquedaComponente" type="integer" column="busqueda_idbusqueda_componente" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="etiqueta" type="string" column="etiqueta" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etiquetax" type="string" column="etiquetax" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etiquetay" type="string" column="etiquetay" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipoGrafico" type="integer" column="tipo_grafico" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">1,torta;2,barras</option>
      </options>
    </field>
    <field name="ancho" type="integer" column="ancho" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">300</option>
      </options>
    </field>
    <field name="alto" type="integer" column="alto" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">400</option>
      </options>
    </field>
    <field name="direccionTitulo" type="integer" column="direccion_titulo" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="orden" type="integer" column="orden" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="indicadorIdindicador" type="integer" column="indicador_idindicador" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
